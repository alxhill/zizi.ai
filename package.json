{
  "name": "zizi",
  "productName": "The Zizi Show",
  "description": "Drag Queens, Drag Kings, Drag Things and Artificial Intelligence",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "homepage": "./",
  "author": "Alexander Hill & Jake Elwes",
  "main": "public/electron.js",
  "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
  "splashscreen": {
    "imageFile": ""
  },
  "executable": "",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^7.2.1",
    "electron-is-dev": "^2.0.0",
    "history": "^5.0.0",
    "hls.js": "^0.14.13",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-scrollable-anchor": "^0.6.1",
    "react-youtube": "^7.11.3"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "public/icons/icon.png"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build",
    "app:dir": "electron-builder --dir",
    "electron-packonly": "electron-builder -c.extraMetadata.main=electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "appId": "your.id",
  "devDependencies": {
    "electron": "^17.1.0"
  }
}
